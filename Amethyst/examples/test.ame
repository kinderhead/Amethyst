namespace example;

struct vec {
    int x;
    int y;
    int z;

    vec(int x, int y, int z) {
        this.x = x;
        this.y = y;
        this.z = z;
    }

    void test() {
        print(this.x);
        this.x += 10;
    }
}

#load
void main() {
    vec v = vec(1, 2, 3);
    v.test();
    print(v);
}

