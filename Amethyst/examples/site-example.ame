namespace example;

#load
void main() {
    var entity = "sheep";
    var targets = @e[type=entity,limit=10];
    var arr = [1, 2, 3, 4];

    if (sum(arr) > 3) as (targets) {
        move-to(7, 7);
    }
}

int sum(int[] arr) {
    int sum = 0;

    for (int i = 0; i < arr.size(); ++i) {
        sum += arr[i];
    }

    return sum;
}

int default_height = 6;

void move-to(int x, int z) {
    @/tp @s $(x) $(default_height + 2) $(z)
}