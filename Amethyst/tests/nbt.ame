namespace test;

struct vec {
    int x;
    int y;
    int z;
}

#test
void test_nbt() {
    start("nbt");
    
    nbt obj;
    obj.x = 100;
    assert(obj.x == 100);

    complete();
}

#test
void test_static() {
    start("nbt static");
    
    vec obj = { x: 100 };
    obj.y = 7;

    assert(obj.x == 100);
    assert(obj.y == 7);
    assert(obj.z == 0);

    complete();
}

#test
void test_nbt_literal() {
    start("nbt literal");
    
    nbt obj = { x: 100 };
    assert(obj.x == 100);

    complete();
}
