namespace test;

struct vec {
    int x;
    int y;
    int z;

    int sum() {
        return this.x + this.y + this.z;
    }
}

#test
void test_nbt() {
    start("nbt");
    
    nbt obj;
    obj.x = 100;
    assert(obj.x == 100);

    complete();
}

#test
void test_static() {
    start("nbt static");
    
    vec obj = { x: 100 };
    obj.y = 7;

    assert(obj.x == 100);
    assert(obj.y == 7);
    assert(obj.z == 0);
    assert(obj.sum() == 107);

    complete();
}

#test
void test_nbt_literal() {
    start("nbt literal");
    
    nbt obj = { x: 100 };
    assert(obj.x == 100);

    complete();
}
