namespace test;

#test
void test_int_list_add() {
    start("int list add");

    int[] x;
    int y = 6;

    x.add(10);
    x.add(y);
    x.add(y * 2);
    x.add(7);

    assert_eq(x[0], 10);
    assert_eq(x[1], 6);
    assert_eq(x[2], 12);
    assert_eq(x[3], 7);
    assert_eq(x.size(), 4);
    
    complete();
}

#test
void test_int_list_index() {
    start("int list index");

    int[] x;
    int y = 6;

    x.add(10);
    x.add(y);
    x.add(y * 2);

    int i = 0;
    assert_eq(x[i], 10);

    i = i + 1;
    assert_eq(x[i], 6);

    i = i + 1;
    assert_eq(x[i], 12);
    
    complete();
}

#test
void test_int_list_literal() {
    start("int list literal");

    int y = 6;
    int[] x = [10, y, y * 2, 7];

    assert_eq(x[0], 10);
    assert_eq(x[1], 6);
    assert_eq(x[2], 12);
    assert_eq(x[3], 7);
    assert_eq(x.size(), 4);
    
    complete();
}

#test
void test_int_list_ref() {
    start("int list ref");

    int[][] arr;
    arr.add([1,2,5]);
    arr[0].add(7);

    int i = 0;
    arr[i].add(-90);

    assert_eq(arr[0][0], 1);
    assert_eq(arr[0][1], 2);
    assert_eq(arr[0][2], 5);
    assert_eq(arr[0][3], 7);
    assert_eq(arr[0][4], -90);
    
    complete();
}
