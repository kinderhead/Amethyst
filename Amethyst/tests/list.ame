namespace test;

#test
void test_int_list_add() {
    start("int list add");

    int[] x;
    int y = 6;

    x.add(10);
    x.add(y);
    x.add(y * 2);

    assert(x[0] == 10);
    assert(x[1] == 6);
    assert(x[2] == 12);
    assert(x.size() == 3);
    
    complete();
}

#test
void test_int_list_index() {
    start("int list index");

    int[] x;
    int y = 6;

    x.add(10);
    x.add(y);
    x.add(y * 2);

    int i = 0;
    assert(x[i] == 10);

    i = i + 1;
    assert(x[i] == 6);

    i = i + 1;
    assert(x[i] == 12);
    
    complete();
}

#test
void test_int_list_literal() {
    start("int list literal");

    int y = 6;
    int[] x = [10, y, y * 2, 7];

    assert(x[0] == 10);
    assert(x[1] == 6);
    assert(x[2] == 12);
    assert(x[3] == 7);
    assert(x.size() == 4);
    
    complete();
}
