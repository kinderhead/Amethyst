namespace amethyst;

inline void core/index-list-get(macro nbt* dest, macro nbt* src, macro int index) {
    @/data modify $(dest) set from $(src)[$(index)]
}

inline void core/index-list-set(macro nbt* src, macro int index, macro nbt value) {
    @/data modify $(src)[$(index)] set value $(value)
}
